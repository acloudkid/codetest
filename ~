echo -n "Enter Passphrase for Encrypted Key and Secret[ENTER]:"
read DECRYPT_PASS
export DECRYPT_PASS
echo -n "Enter AWS Access Key [ENTER]:"
read accesskey
echo $accesskey >> accesskey
export AWS_ENCRYPT_KEY=$(openssl aes-256-cbc -salt -a -e -in accesskey -pass pass:$DECRYPT_PASS)
rm accesskey
#
echo -n "Enter AWS Access Secret [ENTER]:"
read accesssecret
echo $accesssecret >> accesssecret
export AWS_ENCRYPT_SECRET=$(openssl aes-256-cbc -salt -a -e -in accesssecret -pass pass:$DECRYPT_PASS)
rm accesssecret
